    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-fbjs-opensource": "^1.0.0",
    "jest": "^19.0.2",
    "vscode-languageserver-types": "^3.2.0"
  },
  "dependencies": {
    "flow-bin": "^0.44.1",
